{
    "name": "n",
    "displayName": "N",
    "description": "Support for the N programming language by the N building",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "n",
            "aliases": ["N", "n"],
            "extensions": [".n"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "n",
            "scopeName": "source.n",
            "path": "./syntaxes/n.tmLanguage.json"
        }]
    }
}